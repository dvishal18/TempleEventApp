<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appproj.vd.shyambaba">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutusActivity"
            android:label="@string/AboutUsActivity" />
        <activity
            android:name=".EventActivity"
            android:label="@string/EventActivity" />
        <activity
            android:name=".activity.GalleryActivity"
            android:label="@string/GalleryActivity" />
        <activity
            android:name=".WebsiteActivity"
            android:label="@string/WebsiteActivity" />
        <activity
            android:name=".YoutubevideoActivity"
            android:label="@string/VideosActivity" />
        <activity android:name=".MainvideoActivity"></activity>
        <activity
            android:name=".ChannelActivity"
            android:label="@string/VideosActivity"></activity>
        <activity android:name=".VideoActivity"></activity>
        <activity
            android:name=".MainEventActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".DisplayData"
            android:label="@string/EventActivity" />
        <activity
            android:name=".UpdateData"
            android:label="@string/EventActivity" />
        <activity android:name=".CalenderActivity"
            android:label="@string/CalenderActivity"></activity>
    </application>

</manifest>